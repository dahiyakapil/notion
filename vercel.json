{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.js", "use": "@vercel/static" },
    { "src": "api/notion.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/main.js", "dest": "/main.js" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
